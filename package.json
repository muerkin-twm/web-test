    "name": "web-test",
    "version": "0.0.0",
    "description": "Web automation framework that runs with webdriver.io and cucumber",
    "scripts": {
        "clean": "rm -rf ./node_modules yarn.json && yarn install",
        "test": "wdio",
        "test:qa": "yarn test wdio.QA.conf.js",
        "test:single": "yarn test --spec $0",
        "test:tag:smoke": "yarn test --cucumberOpts.tagExpression='@Smoke'",
        "prettier": "prettier --write ."
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-export-default-from": "^7.8.3",
        "@babel/preset-env": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@types/cucumber": "^6.0.1",
        "@wdio/cli": "^6.0.8",
        "@wdio/config": "^6.0.8",
        "@wdio/cucumber-framework": "^6.0.8",
        "@wdio/local-runner": "^6.0.8",
        "@wdio/selenium-standalone-service": "^6.0.8",
        "@wdio/spec-reporter": "^6.0.8",
        "@wdio/static-server-service": "^6.0.8",
        "@wdio/sync": "^6.0.8",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.0.1",
        "chromedriver": "^85.0.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-wdio": "^6.0.8",
        "http-server": "^0.12.1",
        "jest": "^26.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "2.1.2",
        "wdio-chromedriver-service": "^6.0.3",
        "webdriverio": "^6.0.8"
    }
}
